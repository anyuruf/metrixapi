  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@neo4j/graphql": "^1.0.0",
    "@neo4j/graphql-ogm": "^1.0.3",
    "apollo-server": "^2.19.2",
    "apollo-server-lambda": "^2.19.0",
    "bcrypt": "^5.0.1",
    "csv-parse": "^4.10.1",
    "dotenv": "^7.0.0",
    "graphql": "^15.5.1",
    "jsonwebtoken": "^8.5.1",
    "neo4j-driver": "^4.2.2",
    "node-fetch": "^2.6.0",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
